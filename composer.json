{
    "name": "maxleaver/php-haiku",
    "description": "Converts text into a haiku when possible",
    "keywords": ["php", "haiku", "poetry"],
    "license": "MIT",
    "authors": [
        {
            "name": "Max Leaver",
            "email": "maxleaver@gmail.com",
            "homepage": "http://www.maxleaver.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/maxleaver/Text-Statistics"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "davechild/textstatistics": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "PhpHaiku\\": "src/PhpHaiku"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}